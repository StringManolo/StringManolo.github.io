        if (window.requestIdleCallback) {
            requestIdleCallback(function () {
                Fingerprint2.get(function (components) {
                    saveOnDB(components) // an array of components: {key: ..., value: ...}
                })
            })
        } else {
            setTimeout(function () {
                Fingerprint2.get(function (components) {
                    saveOnDB(components) // an array of components: {key: ..., value: ...}
        })
            }, 500)
        }

        function saveOnDB(txt) {
            // console.log(MD5(btoa(JSON.stringify(txt))));
            var fingerprint = MD5(btoa(JSON.stringify(txt)));

            console.log(fingerprint);

            if (navigator.geolocation) {
                navigator.geolocation.getCurrentPosition(function(pos) { storeUbication(pos, fingerprint); });
                // navigator.geolocation.getCurrentPosition(test);
            } else {
                console.log("Geolocation not supported in this browser.");
            }
        }

        function storeUbication(pos, fingerprint) {
            console.log("doing ajax");
            $.ajax({
                url: "api.php",
                method: 'POST',
                data: {'action': 'store_ubication', 'longitude': pos.coords.longitude, 'latitude': pos.coords.latitude, 'fingerprint': fingerprint},
                success: function(result){
                    console.log(result);
                }});
        }
        
